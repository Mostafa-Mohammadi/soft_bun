    "chai": "^4.3.7",
    "concurrently": "^7.6.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "gts": "^3.1.1",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
