{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "./dist",
    "noEmitOnError": false,
    "sourceMap": true,
    "suppressImplicitAnyIndexErrors": true,
    "strictNullChecks":false,
    "strictPropertyInitialization": false,
    "ignoreDeprecations": "5.0"
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}
